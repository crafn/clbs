def buildInfo(env):
	build = {
		"debug": {
			"defines": [],
			"flags": ["g", "O0"]
		},
		"release": {
			"defines": [ "NDEBUG" ],
			"flags": [ "O2" ]
		}
	}
	p= Project()
	p.name= "hello"
	p.src= [ "./src/main.cpp" ]
	p.flags= build[env.tag]["flags"]
	p.defines= build[env.tag]["defines"]
	return p
